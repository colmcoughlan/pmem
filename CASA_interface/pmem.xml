         <value>0</value>
    </param>
    
    <param type="double" name="rmsU">
      <description>rms theorical U</description>
         <value>0</value>
    </param>
    
    <param type="double" name="rmsV">
      <description>rms theorical V</description>
         <value>0</value>
    </param>
    
    <param type="int" name="nbrinter">
      <description>number of interation</description>
         <value>0</value>
    </param>
    
    <param type="double" name="bmaj">
      <description>bmaj</description>
         <value>0</value>
    </param>
   
    <param type="double" name="bmin">
      <description>bmin</description>
         <value>0</value>
    </param>
   
    <param type="double" name="bpa">
      <description>bpa</description>
         <value>0</value>
    </param>
   
    <param type="int" name="blcx">
      <description>blcx</description>
         <value>0</value>
    </param>
   
    <param type="int" name="blcy">
      <description>blcy</description>
         <value>0</value>
    </param>
   
    <param type="int" name="trcx">
      <description>trcx</description>
         <value>0</value>
    </param>
   
    <param type="int" name="trcy">
      <description>trcy</description>
         <value>0</value>
    </param>
   
    <param type="double" name="accelfactor">
      <description>acceleration factor</description>
         <value>0</value>
    </param>
   
    <param type="double" name="qfactor">
      <description>q factor</description>
         <value>0</value>
    </param>
   
    <param type="double" name="polfactor">
      <description>polarisation factor</description>
         <value>0</value>
    </param>
   
    <param type="string" name="outputname">
      <description>output name</description>
         <value></value>
    </param>
   
    <param type="int" name="ignoreedgepixels">
      <description>ignore edge pixels</description>
         <value>0</value>
    </param>
   
    <param type="boolean" name="debug">
      <description>debug mode option</description>
         <value>False</value>
    </param>


  

  </input>

  <returns type="void"/>


